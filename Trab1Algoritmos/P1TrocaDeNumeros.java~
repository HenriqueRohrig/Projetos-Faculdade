public class TrocaDeNumeros {
  public static void main (String args []) {   
    
    int Num;
    int I;
    int B;
    
    Num = Entrada.leiaInt ("Digite um valor: ");
    
    if (Num > 0){
      System.out.println(I = Num);
    }else{
      System.out.println(B = Num);
    }
    System.out.println("O Valor é: ");
    System.out.println(Num); //Não entendi o que era pra ser dito nessa questão.
}
}
        
      
     