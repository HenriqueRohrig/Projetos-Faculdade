public class Div3 {
  public static void main (String args[]) {
    
   int nf,a,b,i;
   
   i=0;
   
   while (i<10){
     
     nf=Entrada.leiaInt("Digite um número:");
     
     if (nf % 3 == 0){
       a=nf;
         b=a+a;
     i++;
  System.out.println("Soma dos números divisíveis por 3: "+b);
     }
   }
  }
}
